<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- This Menu Item will appear in the Upper bar, that's why It needs NO parent or action -->
    <menuitem 
        id="toko_root" 
        name="TOKO" 
        sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem 
        id="toko_barang_categ" 
        name="Barang" 
        parent="toko_root" 
        sequence="10"/>

    <!-- This Menu Item Must have a parent -->
    <menuitem 
        id="toko_partner_categ" 
        name="Partner" 
        parent="toko_root" 
        sequence="20"/>

</odoo>
